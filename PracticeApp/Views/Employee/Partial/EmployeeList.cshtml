<div ng-controller="employeeListController as vm" ng-init="vm.onInitialize()">
    <md-content>
        <md-list>
            <md-divider></md-divider>
            <md-subheader class="md-no-sticky">Employee List</md-subheader>
            <md-divider></md-divider>
            <md-list-item class="md-3-line" ng-repeat="employee in vm.employees">
                <div class="md-list-item-text" layout="column">
                    <h3 ng-bind="employee.id"></h3>
                    <h4 ng-bind="employee.firstName + ' ' + employee.lastName"></h4>
                    <p ng-bind="employee.role"></p>
                </div>
                <md-divider></md-divider>
            </md-list-item>
        </md-list>
        <md-button class="md-raised md-primary" ui-sref="add">Add</md-button>
    </md-content>
</div>